{
    "name": "hyvor/laravel-e2e",
    "description": "Helpers for Laravel E2E testing",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hyvor\\LaravelPlaywright\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hyvor\\LaravelPlaywright\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Supun",
            "email": "supun@hyvor.com"
        }
    ],
    "require": {
        "php": "^8.2 || ^8.3",
        "doctrine/dbal": "^3.6"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.6",
        "phpstan/phpstan": "^2.0",
        "larastan/larastan": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hyvor\\LaravelPlaywright\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
